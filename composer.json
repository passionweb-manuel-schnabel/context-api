{
  "name": "passionweb/context-api",
  "license": "GPL-3.0",
  "description": "Shows example usages of the TYPO3 Context API. (TYPO3 CMS)",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^11.5 | ^12.4"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\ContextApi\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/context-api": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "context_api"
    }
  }
}
